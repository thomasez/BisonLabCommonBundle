{
    "name": "bisonlab/common-bundle",
    "type": "symfony-bundle",
    "description": "This bundle includes some of the typical stuff the author needs for his projects, like a user object, generic REST stuff and the context system.",
    "keywords": ["common symfony user context rest"],
    "homepage": "https://github.com/thomasez/BisonLabCommonBundle",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Thomas Lundquist",
            "email": "github@bisonlab.no"
        }
    ],
    "require": {
        "symfony/framework-bundle": ">=2.8",
        "friendsofsymfony/user-bundle": "2.0.*@dev"
    },
    "suggest": {
        "robloach/component-installer": "*",
        "components/jquery": "^1.11.0",
        "components/normalize.css": "3.*",
        "vaibhavpandeyvpz/doctrine-datatables": "dev-master",
        "datatables/datatables": "^1.10",
        "components/bootstrap": "3.*"
    },
    "config": {
        "component-dir": "web/public",
        "component-baseurl": "/assets",
        "component-scriptFilters": {
            "\\Assetic\\Filter\\GoogleClosure\\CompilerApiFilter": []
        },
        "component-styleFilters": {
            "\\Assetic\\Filter\\CssImportFilter": []
        }
    },
    "autoload": {
        "psr-4": { "BisonLab\\CommonBundle\\": "" }
    }
}
